{
  "name": "@gekichumai/dxrating-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env-shell VITE_BUILD_PLATFORM=web \"vite\"",
    "build": "echo 'WARNING: Use yarn build:web to build for web instead, or use yarn build:app to build for app instead. Building for web...' && yarn build:web",
    "build:base": "tsc && vite build",
    "build:app": "cross-env-shell VITE_BUILD_PLATFORM=app \"yarn build:base && rm -rf dist/js/*.map && capacitor copy\"",
    "build:web": "cross-env-shell VITE_BUILD_PLATFORM=web \"yarn build:base && echo 'Artifacts built under dist/'\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy:ios": "capacitor sync ios",
    "gen:supabase": "supabase gen types typescript --project-id lbtpnmdffuimikksrvns > src/models/supabase.types.ts"
  },
  "dependencies": {
    "@capacitor/core": "^5.5.0",
    "@capacitor/ios": "^5.5.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@gekichumai/dxdata": "*",
    "@gekichumai/oneshot": "*",
    "@mui/material": "^5.14.13",
    "@sentry/react": "^7.98.0",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-table": "^8.10.7",
    "@unocss/preset-mini": "^0.58.4",
    "@unocss/preset-wind": "^0.58.4",
    "@unocss/reset": "^0.58.4",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "fuse.js": "^6.6.2",
    "i18next": "^23.7.20",
    "i18next-browser-languagedetector": "^7.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.0.1",
    "react-use": "^17.4.0",
    "react-virtuoso": "^4.6.1",
    "satori": "^0.10.9",
